{
  "name": "@depot/cli",
  "version": "1.0.0",
  "description": "depot CLI",
  "main": "./dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist",
    "prestart": "pnpm run build",
    "start": "node ./dist/index.js",
    "test": "jest",
    "dev": "tsc --watch",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint": "tsc --noEmit && prettier --check \"src/**/*.{js,jsx,ts,tsx}\"",
    "typecheck": "tsc --noEmit"
  },
  "author": "Francois Laubscher",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@types/node-fetch": "^2.6.9",
    "jest": "^29.7.0",
    "prettier": "^3.4.0",
    "rimraf": "^5.0.5",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@depot/core": "workspace:*",
    "node-fetch": "^2.7.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
